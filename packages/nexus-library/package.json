{
  "name": "nexus-library",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build:css && vite build",
    "prepare": "panda codegen",
    "build:css": "panda cssgen --outfile src/styles.css"
